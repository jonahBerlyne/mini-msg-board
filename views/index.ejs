<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% messages.forEach(message => { %>
      <h3><%= message.user %></h3>
      <h4><%= message.text %></h4>
      <h5><%= message.added %></h5>
    <% }); %>
    <a href="/new">Write a new message</a>
  </body>
</html>
